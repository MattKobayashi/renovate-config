{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "assignees": ["MattKobayashi"],
  "extends": ["config:recommended"],
  "packageRules": [
    {
      "matchPackageNames": ["fireflyiii/core"],
      "versioning": "regex:^version-(?<major>\\d+)\\.(?<minor>\\d+)\\.?(?<patch>\\d+)?$"
    },
    {
      "matchPackageNames": ["linuxserver/jellyfin"],
      "allowedVersions": "<2021"
    },
    {
      "matchPackageNames": ["linuxserver/lidarr"],
      "allowedVersions": "<8"
    },
    {
      "matchPackageNames": ["linuxserver/netbootxyz"],
      "allowedVersions": "<2"
    },
    {
      "matchPackageNames": ["linuxserver/overseerr"],
      "allowedVersions": "<2021"
    },
    {
      "matchPackageNames": ["linuxserver/qbittorrent"],
      "allowedVersions": "<14"
    },
    {
      "matchPackageNames": ["linuxserver/tautulli"],
      "allowedVersions": "<2021"
    }
  ]
}
